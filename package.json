    "@babel/node": "^7.27.1",
    "@babel/preset-env": "^7.27.1",
    "@slack/web-api": "^7.9.1",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "cypress": "^14.3.3",
    "dotenv": "^16.5.0",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "mongodb": "^6.16.0",
    "mongoose": "^8.14.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.27.1",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.31.0",
    "nodemon": "^3.1.10"
