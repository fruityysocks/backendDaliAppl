{
  "name": "express-babel-eslint-node-starter",
  "version": "7.0.0",
  "description": "* node with babel * expressjs * airbnb eslint rules",
  "author": "CS52",
  "main": "src/server.js",
  "scripts": {
    "test": "eslint src",
    "prodtest": "cypress run",
    "start": "nodemon src/server.js --exec babel-node",
